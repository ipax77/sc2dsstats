@page "/stats"
@using pax.dsstats.shared;

<PageTitle>Stats</PageTitle>

<sc2dsstats.razorlib.Stats.StatsComponent StatsRequest="statsRequest"></sc2dsstats.razorlib.Stats.StatsComponent>

@code {
    StatsRequest statsRequest = new()
    {
        StatsMode = StatsMode.Winrate,
        StartTime = new DateTime(2021, 1, 1),
        EndTime = DateTime.Today,
        Uploaders = false,
        DefaultFilter = true
    };
}
